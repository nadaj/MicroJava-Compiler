program MJProgram
	int x, m;
	
	const bool flag = false;
	const bool flagt = true;
	
	{
		int k()int c;
		{
			c = 0;
			print(0);
			return c + 1;
		}
		
		int inkrem(int i)
		{
			return i-1;
		}
		
		void main()int y[];char t[];bool r[];int i;char u;bool v;
		{
			x = 3;
			m = -1;
			print(k());	// 0 i 1
			x = inkrem(x);
			print(x);	//2
			x = -x + m;	//-3
			print(x);
			x = x * 2;
		}
	}