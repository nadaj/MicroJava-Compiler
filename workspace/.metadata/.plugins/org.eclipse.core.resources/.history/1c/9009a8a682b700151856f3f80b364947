program MJProgram
	int x;
	
	const bool flag = false;
	const bool flagt = true;
	
	{
		int k()int c;
		{
			c = 0;
			print(0);
			return c + 3;
		}
		
		void main()int y[];char t[];bool r[];int i;char u;bool v;
		{
			x = 3;
			print(111);
			print('c');
			k();
			print(x);
			print(flag);
			y = new int[3];
			y[0] = 1;
			y[1] = 2;
			y[2] = 3;
			print(y[0]);
			print(y[1]);
			print(y[2]);
			t = new char[5];
			t[0] = 'l';
			t[1] = 'a';
			t[2] = 'v';
			t[3] = 'i';
			t[4] = 'c';
			print(t[0],1);
			print(t[1],2);
			print(t[2],3);
			print(t[3],4);
			print(t[4],5);
			r = new bool[3];
			r[0] = flag;
			r[1] = flagt;
			r[2] = flag;
			print(r[0]);
			print(r[1]);
			print(r[2]);
			// STAMPA 111c    0    30    1    2    3lavic010
			x++;		//bilo je 3
			print(x);
			x--;		//bilo je 4
			print(x);
			read(x);	//int
			print(x);
			read(u);	//char
			print('v');
			print(u);
			read(v);	//bool
			print(v);
			read(y[0]);
			read(t[0]);
			read(r[0]);
			print(y[0], 2);
			print(t[0], 2);
			print(r[0], 2);
		}
	}