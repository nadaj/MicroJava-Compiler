program MJProgram
	class A{
		int x[],y[];
	}
	
	
	class B extends A          {
		int i;int a; 
		{
			int getValue(int a)int b; bool c;
			{ return this.i + this.x[0] + a; }
		}
	}
	
	class C {
		A theA;
		int x[];
		int i;
	}
	const string nekistring = "blabla";
	const string nestotamo = "nestonesto";
	
	int x, y,e,g ;
   string dt[] ;
{
	int inkrem(int a)
	{
		a++;
		return a;
	}
	
	void main() A a; C c,r, u[]; int i, x[],g; string n;
	{
		n = nekistring;
		a = new A;
		a.x = new int[5];
		a.y = new int[5];
		u = new C[5];
		c = new C;
		c.theA = new A;
		u[0].theA = new A;
		u[0].theA.x = 5;
		{
		c.theA = a;
		c.theA.x = new int[3];
		c.x = new int[5];
		}
		x = new int[3];
		i=0;
		read(c.i);
		while(i<5) {
			read(c.x[i]); 
			read(c.theA.x[i]);
			inkrem(i);
		}
		print(c.getValue(c.theA.x[0]));
	}
}
