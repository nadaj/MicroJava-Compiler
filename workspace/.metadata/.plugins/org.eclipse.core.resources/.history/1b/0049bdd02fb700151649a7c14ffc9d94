program MJProgram
	int x, y;
	
	const bool flag = false;
	{
		int k()int c;
		{
			c = 0;
			print(0);
			return c + 3;
		}
		
		void main()int y[];char t[];
		{
			x = 3;
			print(111);
			print('c');
			k();
			print(x);
			print(flag);
			y = new int[3];
			y[0] = 1;
			y[1] = 2;
			y[2] = 3;
			print(y[0]);
			print(y[1]);
			print(y[2]);
			t = new char[5];
			t[0] = 'l';
			t[1] = 'a';
			t[2] = 'v';
			t[3] = 'i';
			t[4] = 'c';
			print(t[0]);
			print(t[1]);
			print(t[2]);
			print(t[3]);
			print(t[4]);
		}
	}